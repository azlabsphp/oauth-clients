{
    "name": "drewlabs/authorized-clients",
    "description": "Provides core interfaces for working with server authorized clients, along with a generic client validator class",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "http",
        "authorized server clients",
        "clients scopes"
    ],
    "autoload": {
        "psr-4": {
            "Drewlabs\\AuthorizedClients\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\AuthorizedClients\\Tests\\": "tests/"
        }
    },
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "psr/http-client": "^1.0",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0"
        
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "suggest": {
        "nyholm/psr7": "Required to create psr7 requests",
        "nyholm/psr7-server": "Required to create psr7 server requests",
        "symfony/psr-http-message-bridge": "Symfony Psr7 request bridge for transforming symfony request into psr7 ^2.1"
    }
}
